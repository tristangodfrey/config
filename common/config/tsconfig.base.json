{
  "compilerOptions": {
	"experimentalDecorators": true,
	"target": "ESNext",
	"module": "ESNext",
	"moduleResolution": "bundler",
	"declaration": true,
	"declarationMap": true,
	"sourceMap": true,
	"strict": true,
	"noImplicitAny": false,
	"strictNullChecks": false,
	"allowSyntheticDefaultImports": true,
	"esModuleInterop": true,
	"emitDecoratorMetadata": true,
	"skipLibCheck": true,
	"types": ["bun-types"]
  }
}
